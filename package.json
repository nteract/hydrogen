{
  "name": "Hydrogen",
  "main": "./lib/main",
  "version": "0.12.4",
  "description": "Run code and get results inline using Jupyter kernels like IPython, IJulia, and iTorch",
  "author": "Will Whitney <me@willwhitney.com>",
  "keywords": [
    "execute",
    "run",
    "jupyter",
    "ipython",
    "julia",
    "python",
    "torch",
    "ijulia",
    "itorch",
    "plot",
    "image"
  ],
  "activationCommands": {
    "atom-text-editor": [
      "hydrogen:select-kernel",
      "hydrogen:connect-to-remote-kernel",
      "hydrogen:run",
      "hydrogen:run-and-move-down",
      "hydrogen:run-all",
      "hydrogen:run-all-above",
      "hydrogen:run-cell",
      "hydrogen:run-cell-and-move-down"
    ]
  },
  "scripts": {},
  "repository": "https://github.com/nteract/hydrogen",
  "license": "MIT",
  "engines": {
    "atom": ">=1.6.0 <2.0.0"
  },
  "dependencies": {
    "atom-message-panel": "^1.2.4",
    "atom-space-pen-views": "^2.0.5",
    "escape-string-regexp": "^1.0.5",
    "jmp": "0.4.x",
    "jupyter-js-services": "^0.18.0",
    "lodash": "^4.14.0",
    "requirejs": "^2.2.0",
    "spawnteract": "^2.1.1",
    "transformime": "^3.1.0",
    "transformime-marked": "0.0.1",
    "uuid": "^2.0.1",
    "ws": "^1.1.1",
    "xmlhttprequest": "^1.8.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provide"
      }
    }
  },
  "devDependencies": {
    "coffeelint": "^1.15.7"
  }
}
