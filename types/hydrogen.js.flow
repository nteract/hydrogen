export type HydrogenCellType = 'markdown' | 'codecell';

export type HydrogenCell = {
  start: atom$Point,
  end: atom$Point,
  range: atom$Range,
  cellType: HydrogenCellType,
  code: string
}

export type Kernelspec = {
  env: Object,
  argv: Array<string>,
  display_name: string,
  language: string
};

// Be more specific
export type Message = {
  header: Object,
  parent_header: Object,
  content: Object
};

// We can't put it in store/index.js because Atom's babel version doesn't support this syntax
export type KernelObj = { [string]: Kernel };
export type KernelMapping = Map<string, Kernel | KernelObj>;
